	"tokenColors": [
		{
			"settings": {
				"background": "#00313d",
				"caret": "#005c50",
				"foreground": "#F8F8F8",
				"invisibles": "#016e60",
				"lineHighlight": "#000000",
				"selection": "#004a5c"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"background": "#00313d",
				"foreground": "#F8F8F8"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#018776"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#46656d"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#9fdea1"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "",
				"foreground": "#eb934c"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#01d0b6"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#95a0a3"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"fontStyle": "",
				"foreground": "#8e9198"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#af6b2d"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#01556a",
				"foreground": "#01d0b6"
			}
		},
		{
			"name": "Embedded Source",
			"scope": "text source",
			"settings": {
				"background": "#af2f33"
			}
		},
		{
			"name": "Embedded Source (Bright)",
			"scope": "text.html.ruby source",
			"settings": {
				"background": "#af2f33"
			}
		},
		{
			"name": "Entity inherited-class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#ff5d0d"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#8ebbb5"
			}
		},
		{
			"scope": [
				"string constant",
				"constant.character.escape"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#eb934c"
			}
		},
		{
			"name": "String.regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#eb934c"
			}
		},
		{
			"name": "String variable",
			"scope": "string variable",
			"settings": {
				"foreground": "#4e5fa4"
			}
		},
		{
			"name": "Support.function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#eb934c"
			}
		},
		{
			"name": "Support.constant",
